{
  "name": "quick-data-reporter",
  "version": "0.1.0",
  "description": "Quick Data Reporter",
  "private": true,
  "author": {
    "email": "stocki87@gmail.com",
    "name": "stockbal"
  },
  "scripts": {
    "lint": "eslint dev",
    "build:js": "babel dev --out-dir webapp  --source-maps true --extensions \".js\" --copy-files",
    "compile:less": "less-watch-compiler dev/css webapp/css main.less --run-once",
    "watch:js": "babel dev --out-dir webapp --source-maps inline --extensions \".js\" --copy-files --watch",
    "watch:less": "less-watch-compiler dev/css webapp/css main.less",
    "deploy:ui5": "ui5 build --include-task=generateManifestBundle generateCachebusterInfo --clean-dest",
    "build:ui5": "ui5 build --include-task=generateManifestBundle generateCachebusterInfo buildThemes --exclude-task=ui5-task-nwabap-deployer --clean-dest",
    "start:ui5": "ui5 serve --port 1081 --open \"index.html?sap-language=EN\"",
    "start:ui5_mock": "ui5 serve --config ui5-mock.yaml --port 1081 --open \"index_mock.html?sap-language=EN\"",
    "build": "npm-run-all clean build:js compile:less build:ui5",
    "clean": "rimraf webapp dist",
    "deploy": "npm-run-all clean build:js deploy:ui5",
    "start": "npm-run-all clean build:js --parallel watch:js watch:less start:ui5",
    "start:mock": "npm-run-all clean build:js --parallel watch:js watch:less start:ui5_mock"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.2",
    "@babel/eslint-parser": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "babel": "^6.23.0",
    "babel-preset-transform-ui5": "^7.0.3",
    "dotenv": "^9.0.2",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "less-watch-compiler": "^1.15.2",
    "npm-run-all": "^4.1.5",
    "parseurl": "1.3.3",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ui5-middleware-livereload": "^0.5.4",
    "ui5-middleware-route-proxy": "1.0.9",
    "ui5-middleware-servestatic": "^0.3.4",
    "ui5-task-nwabap-deployer": "1.0.15"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload",
      "ui5-middleware-route-proxy",
      "ui5-middleware-servestatic",
      "ui5-task-nwabap-deployer"
    ]
  }
}
